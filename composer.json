{
    "name": "ramadan/custom-fresh",
    "description": "A Laravel package to specify the tables that you do not want to drop while refreshing the database.",
    "homepage": "https://github.com/mahmoudmohamedramadan/custom-fresh",
    "keywords": [
        "laravel",
        "custom-fresh",
        "migrate",
        "database",
        "refreshing",
        "mariadb",
        "mssql",
        "mysql",
        "pgsql",
        "postgresql",
        "oracle",
        "pdo",
        "queryobject",
        "sql",
        "sqlite",
        "sqlserver"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Ramadan",
            "email": "mahmoudramadan496@gmail.com",
            "homepage": "https://mmramadan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0|^11.0",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "Ramadan\\CustomFresh\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ramadan\\CustomFresh\\CustomFreshServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
