{
    "name": "ramadan/custom-fresh",
    "description": "A Laravel package to specify the tables that you do not want to drop while refreshing the database",
    "homepage": "https://github.com/mahmoudmohamedramadan/custom-fresh",
    "keywords": [
        "laravel",
        "custom-fresh",
        "migrate",
        "database",
        "refreshing",
        "development"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Mohamed Ramadan",
            "email": "mahmoudramadan496@gmail.com",
            "homepage": "https://www.mmramadan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ramadan\\CustomFresh\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ramadan\\CustomFresh\\CustomFreshServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/mahmoudmohamedramadan/custom-fresh#support-me"
        },
        {
            "type": "other",
            "url": "https://blog.mmramadan.com/support-me"
        }
    ]
}